default {\n    name: \"Filtros\",\n    data: function() {\n        return{\n            carrosFiltrados: [],\n            marca: \"\",\n            numRegistros: \"\",\n            baseURI: \"http://localhost:8080/api/carros\"\n        }\n    },\n    methods: {\n        getByMarca: function (){\n            this.$http\n             .get(this.baseURI + \"/search?marca=\" + this.marca)\n             .then((result) => {\n                this.carrosFiltrados = result.data;\n\n            })\n            .catch(function(error) {\n                console.log(error);\n            });\n        },\n        \n        getByNumRegistros: function (){\n            this.$http\n             .get(this.baseURI + \"/search?quantidade=\" + this.numRegistros)\n             .then((result) => {\n                this.carrosFiltrados = result.data;\n\n            })\n            .catch(function(error) {\n                console.log(error);\n            });\n        }\n    }\n}\n</script>\n\n<style>\n\n</style>"]}]}